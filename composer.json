{
    "name": "it-all/pageflow",
    "type": "library",
    "description": "Pageflow initializes a PHP page controller app with important PHP settings, a custom error handler, optional PHPMailer access, optional PostGreSQL database connection (PHP pgsql extension required).",
    "keywords": [
        "PHP",
        "initializer",
        "framework",
        "micro"
    ],
    "license": "GPL-3.0",
    "authors": [        
        {
            "name": "Greg Catalano",
            "email": "greg@it-all.com",
            "homepage": "https://it-all.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^7.0.0",
        "delight-im/auth": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Pageflow\\": "src/"
        }
    }
}
